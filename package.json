    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
